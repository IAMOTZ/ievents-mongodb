{
  "name": "ievents",
  "version": "1.0.0",
  "description": "A web app to help manage and run commercial event centers",
  "main": "index.js",
  "scripts": {
    "start": "forever ./build/app.js",
    "prod:server": "babel ./server -d ./build",
    "prod:client": "set NODE_ENV=production&& webpack",
    "dev:server": "npm run database:setup && nodemon ./server/app --exec babel-node  --watch ./server",
    "dev:client": "webpack-dev-server --content-base src --inline --hot",
    "dev:template": "gulp",
    "test:server": "export NODE_ENV=test && npm run database:setup && nyc mocha --compilers js:babel-core/register --require babel-polyfill ./server/test/testPath.js --timeout 5000 --exit",
    "test:client": "jest && codecov",
    "test:client:update": "jest -u",
    "test:e2e": "nightwatch",
    "test:e2e:setup": "selenium-standalone install",
    "test:full": "npm run test:server && npm run test:client",
    "lint": "./node_modules/.bin/eslint",
    "coverage": "npm run test:server && nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "npm run prod:client && npm run prod:server && npm run database:setup",
    "database:setup": "babel-node ./server/setUpDatabase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IAMOTZ/Ievents.git"
  },
  "author": "Ogunniyi Tunmise",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IAMOTZ/Ievents/issues"
  },
  "homepage": "https://github.com/IAMOTZ/Ievents#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0-beta.2",
    "classnames": "^2.2.5",
    "cloudinary": "^1.19.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "forever": "^0.15.3",
    "history": "^4.7.2",
    "image-webpack-loader": "^3.4.2",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "mongoose": "^5.8.11",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "node-sass": "^4.13.1",
    "nodemailer": "^4.4.2",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.11.1",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.12.0",
    "react-dropzone": "^4.2.3",
    "react-infinite-calendar": "^2.3.1",
    "react-paginate": "^5.2.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.22.8",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.11.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-remove-decorator": "^1.0.0",
    "browser-sync": "^2.26.7",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "codecov": "^3.6.5",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "moxios": "^0.4.0",
    "nightwatch": "^0.9.19",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1"
  },
  "nyc": {
    "exclude": [
      "server/config/*.js",
      "server/models/*.js",
      "server/setUpDatabase.js",
      "test/**/*.js"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./client/test/testSetup/browserMock.js",
      "./client/test/testSetup/globals.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/client/**/*.{js,jsx}",
      "!**/client/test/**",
      "!**/client/helpers/**",
      "!**/client/store.js",
      "!**/client/index.{js,jsx}",
      "!**/client/reducers/index.js",
      "!**/client/components/Routes.{js,jsx}",
      "!**/client/components/common/Calendar/**.jsx"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
